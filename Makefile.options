endif

ifneq ($(ROWSUM),)
BUILD_OPTIONS += -ROWSUM='$(ROWSUM)'
endif
ifneq ($(OPTION_ROWSUM),)
BUILD_OPTIONS += -ROWSUM='$(OPTION_ROWSUM)'
endif

ifneq ($(DATASIZE),)
