BUILD_OPTIONS :=

ifneq ($(LMDB_DEBUG),)
BUILD_OPTIONS += -LMDB_DEBUG='$(LMDB_DEBUG)'
endif
ifneq ($(OPTION_LMDB_DEBUG),)
