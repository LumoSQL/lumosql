# "rowsum_algorithm" runtime option

# this option changes the default rowsum algorithm to use a different
# one; it only makes sense together with "rowsum-on"

# this is a runtime option
build = no

# value can be one of...
enum = none,empty,sha3_512,sha3_384,sha3_256,sha3_224,sha3
equiv = sha3_256,sha3

# default value if not specified
default = sha3_256

