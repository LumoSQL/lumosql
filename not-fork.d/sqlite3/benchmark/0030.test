set d100 [expr $options(DATASIZE) * 100]

set name "$d100 SELECTs without an index"

for {set i 0} {$i<$d100} {incr i} {
    set lwr [expr {$i*100}]
    set upr [expr {($i+10)*100}]
    append sql "SELECT count(*), avg(b) FROM t2 WHERE b>=$lwr AND b<$upr;\n"
}

if {$d100 == 100} {
    lappend results \
	{^41\|486\.365} \
	{^40\|598\.5} \
	{^40\|698\.5} \
	{^40\|798\.5}
}

